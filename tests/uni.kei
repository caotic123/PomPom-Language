
Static ≡ : (A : *) ~> ~ A ~> ~ A ~> *.
Static refl : (A : *) ~> (n : A) ~> (≡ A n n).

Eq | Set x y :: (A : *) (x : A) (y : A) ~> *  
  => {(≡ Set x y) :: |refl}.

axiom_k 
 |A a T p e :: (A : *) (a : A) (T : (~ (Eq A a a) ~> *)) (p : (T (refl A a))) (e : (Eq A a a)) (T e) =>
  [e of (T e)
     |(refl z x) => p
  ].