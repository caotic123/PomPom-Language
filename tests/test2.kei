eql_prop
  | x y H :: (x : Nat) (y : Nat) ~ (Eq Bool (eql x y) true) ~> (Eq Nat x y) => 
  [x of (Eq Nat x y)
    |Z => [y of (Eq Nat x y)
       |Z => (refl nat Z)
       |(S n) => (⊥ (Eq Nat x y) (bool_false (symmetry Bool false true H)))
    ]
    |(S n) => [y of (Eq Nat x y)
       |Z =>  (⊥ (Eq Nat x y) (bool_false (symmetry Bool false true H)))
       |(S n') => (cong nat n n' nat S (eql_prop n n' H))
    ]
  ].

unique_eql 
