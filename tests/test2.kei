left_succ_nat | x y :: (x : Nat) ~> (y : Nat) ~> {(≡ nat (+ x (S y)) (S (+ x y))) :: | refl } => 
  [x of {(≡ nat (+ x (S y)) (S (+ x y))) :: | refl}
    |Z => (refl nat (S y))
