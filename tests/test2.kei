 sorted_pred
   | xs v H :: (xs : (List Nat)) (v : Nat) ~ (sorted (new Nat v xs)) ~> (sorted xs) => [
     xs of (sorted xs)
      |(empty _) => I
      |(new typ head tail) => [tail of (sorted xs)
        |(empty _) => I
        |(new typ2 x tail2) =>
          def sort_tail = (second (Eq Bool (gte head v) true) (sorted (new typ head tail)) H);
          def sort_tail2 = (second (Eq Bool (gte x head) true) (sorted (new typ2 x tail2)) sort_tail);
          def le_tail2 = (first (Eq Bool (gte x head) true) (sorted (new typ2 x tail2)) sort_tail);
          def rec = (sorted_pred tail2 x sort_tail2);
          (MkPair (Eq Bool (gte x head) true) (sorted tail2) le_tail2 rec)
      ]
   ].

 sorted_aux
 | xs v H :: (xs : (List Nat)) (v : Nat) ~ (sorted xs) ~> (sorted (insert_sort xs v)) => 
   [xs of (sorted (insert_sort xs v))
    |(empty _) => (I :: Unit)
    |(new typ head tail) => [(gte head v) of (sorted (insert_sort xs v))
      |true => 
        (MkPair (Eq Bool true true) (sorted tail) (refl Bool true) (sorted_pred tail head H))
      |false => [tail of (sorted (insert_sort xs v))
        |(new _ head2 tail') => [(gte head2 v) of (sorted (insert_sort xs v))
          |true =>  
            def rec = (sorted_pred tail' head2 (second (Eq Bool (gte head2 head) true) (sorted tail) H));
            def v⩾head = (gte_inverse head v (refl Bool true));
            def projection = (MkPair (Eq Bool true true) (sorted tail') (refl Bool true) rec);
            (MkPair (Eq Bool (gte v head) true) (Pair (Eq Bool true true) (sorted tail')) v⩾head projection)
          |false => 
            def first_projection = (first (Eq Bool (gte head2 head) true) (sorted tail) H);
            def rec = (sorted_pred tail' head2 (second (Eq Bool (gte head2 head) true) (sorted tail) H));
            def rec = (sorted_aux tail' v rec);
            (MkPair (Eq Bool (gte head2 head) true) (sorted (insert_sort tail' v)) first_projection rec)
        ]
        |(empty _) => 
          def v⩾head = (gte_inverse head v (refl Bool true));
          (MkPair (Eq Bool (gte v head) true) Unit v⩾head I)
      ]
    ]
   ].

sorted_proof
  | xs :: (xs : (List Nat)) ~> (sorted (sort xs)) => [xs of (sorted (sort xs))
    |(empty _) => I
    |(new Nat head tail) => (sorted_aux (sort tail) head (sorted_proof tail))
  ].

